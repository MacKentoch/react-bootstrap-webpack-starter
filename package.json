{
  "name": "react-bootstrap-webpack-starter",
  "version": "6.0.0",
  "description": "react js + bootstrap + webpack starter",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/react-bootstrap-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MacKentoch/react-bootstrap-webpack-starter/issues"
  },
  "engines": {
    "node": ">=8.11.1",
    "npm": ">=6.0.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "test": "cross-env NODE_ENV=test jest --no-cache",
    "analyze": "rimraf docs/public/assets && cross-env NODE_ENV=production webpack -p --config webpack.analyze.config.js",
    "dev": "rimraf docs/public/assets && cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "prod": "rimraf docs/public/assets && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack.hot.reload.config.js --mode development",
    "serve-dev": "npm run dev && nodemon ./src/server/server.js",
    "serve-prod": "npm run prod && node ./src/server/server.js"
  },
  "keywords": [
    "react",
    "react 16",
    "ES6",
    "ES7",
    "ES2015",
    "ES2016",
    "bootstrap",
    "starter",
    "webpack",
    "hot-reload",
    "hot reload",
    "webpack 4",
    "webpack",
    "react-router",
    "react-router 4",
    "react-router v4"
  ],
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.0.4",
    "@types/chalk": "^2.2.0",
    "@types/classnames": "^2.2.3",
    "@types/date-fns": "^2.6.0",
    "@types/enzyme": "^3.1.10",
    "@types/express": "^4.0.39",
    "@types/history": "^4.6.2",
    "@types/jest": "^22.2.3",
