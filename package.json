    "prettier-eslint": "^8.8.1",
    "pretty-error": "^2.1.1",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.3.2",
    "react-motion": "^0.5.2",
    "react-overdrive": "^0.0.9",
